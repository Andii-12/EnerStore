# React App Environment Variables
# Copy this file to .env.local for local development
# For Vercel, set these in the Vercel dashboard

# API Base URL (Railway server URL)
REACT_APP_API_URL=https://your-railway-app.railway.app

# Socket.io Server URL (Railway server URL)
REACT_APP_SOCKET_URL=https://your-railway-app.railway.app

# Environment
REACT_APP_ENV=production

# Optional: Analytics (if you add them later)
# REACT_APP_GA_TRACKING_ID=G-XXXXXXXXXX 